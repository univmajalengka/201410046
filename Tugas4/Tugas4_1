{Nama program : perkalian}


program tabel_perkalian;
uses crt;

var n,i:integer;

begin
clrscr;
i:=0;
writeln('Masukan nilai perkalian :');readln(n);
while i<n do
begin
for i:= 1 to 5 do
writeln(n,'*',i+1,'=',(i+1)*n);
i:=i+1
end;
readln;
end.
